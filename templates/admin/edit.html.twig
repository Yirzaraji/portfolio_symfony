{% extends 'base.html.twig' %}


{% block body %}

{% include 'NavBarSimple.html.twig' %}

<div class="container">
    <div class="row justify-content-center">
        <div class="col-12 col-md-9 text-center mt-5 mb-5">
            {{form_start(editPostForm)}}
                <h3 class="titleColor mb-4"><b><i class="fas fa-edit"></i> EDIT POST.</b></h3>
                <i class="fas fa-check"></i> {{ form_row(editPostForm.title)}} 
                <i class="fas fa-check"></i> {{ form_row(editPostForm.technos)}}
                <i class="fas fa-check"></i>{{ form_row(editPostForm.projectImage)}}
                <i class="fas fa-check"></i> {{ form_row(editPostForm.categorie)}}  
            {{form_end(editPostForm)}}
        </div>
    </div>
</div>

{% endblock %}